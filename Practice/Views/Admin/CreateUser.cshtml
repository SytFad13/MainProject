@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<h3>
    Create user page
</h3>
<form asp-controller="Admin" asp-action="CreateUser" method="post">
    <label for="email"><b>Email</b></label>
    <input type="text" name="userName" value="" id="email" />

    <label for="psw"><b>Password</b></label>
    <input type="password" name="password" value="" id="psw" />

    <label for="psw"><b>Repeat password</b></label>
    <input type="password" name="password" value="" id="psw-repeat" />

    <button type="submit" class="btn btn-primary registerbtn"> Create user </button>
</form>

<br>
<a class="btn btn-danger" asp-controller="Admin" asp-action="GetUsers">Return back to users</a>